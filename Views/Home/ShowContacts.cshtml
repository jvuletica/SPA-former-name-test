<!--ShowContacts partial view-->
<table>
    <thead>ContactList</thead>
    <tr>
        <td>Name</td>
        <td>Surname</td>
        <td>Address</td>
    </tr>
    <tr ng-repeat="contact in contacts">
        <td ng-bind="contact.Name"></td>
        <td ng-bind="contact.Surname"></td>
        <td ng-bind="contact.Address"></td>
        <td><button ng-click="Delete(contact.ContactId)">Delete</button></td>
        <td><a href="/#/updatecontact" ng-click="TargetContact(contact)">Update</a></td>
        <td><a href="/#/contactdetails" ng-click="TargetContact(contact)">Details</a></td>
    </tr>
</table>
